{
  "name": "redux-service-middleware",
  "version": "1.0.1",
  "description": "A Redux middleware for easy service integration",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "precommit": "echo 'Running pre-commit hooks...' && npm run test",
    "prebuild": "npm run test",
    "build": "rm -rf dist & npm run build-commonjs & npm run build-es & npm run build-umd & npm run build-umd-min",
    "build-es": "BABEL_ENV=es `npm bin`/babel src -d dist/es",
    "build-commonjs": "`npm bin`/babel src -d dist",
    "build-umd": "`npm bin`/webpack dist/umd/redux-service-middleware.js",
    "build-umd-min": "NODE_ENV=production `npm bin`/webpack dist/umd/redux-service-middleware.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varghese88/redux-service-middleware.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/varghese88/redux-service-middleware/issues"
  },
  "homepage": "https://github.com/varghese88/redux-service-middleware#readme",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-env": "^1.7.0",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
